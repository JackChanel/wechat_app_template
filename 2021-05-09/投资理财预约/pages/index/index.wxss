/**index.wxss**/

/* 标题 */
.title_area {
  position: absolute;
  top: 7%;
  left: 13%;
  z-index: 1;
  width: 72%;
  height: 21%;
}
.title_area .img {
  width: 100%;
  height: 100%;
}

.bottomTitle_area {
  position: absolute;
  top: 80%;
  left: 27%;
  z-index: 1;
  width: 46%;
  height: 18%;
}
.bottomTitle_area .img {
  width: 100%;
  height: 100%;
}

/* 表单 */
.form_area {
  position: absolute;
  top: 51%;
  left: 10%;
  z-index: 10;
  width: 80%;
}
.form_item {
  position: relative;
  margin-bottom: 2.6%;
}
.form_item::after {
  content: '';
  position: absolute;
  left: 0;
  top: 0;
  width: 100px;
}
.form_itemInput {
  padding: 3.6% 8%;
  background: url(data:image/png;base64,) no-repeat 50% 50% / contain;
}
.form_itemInput .input {
  color: #fff;
  font-size: 32rpx;
  font-family: "Microsoft YaHei",arial,verdana,sans-serif;
}
.form_itemSubmit {
  padding-top: 3%;
}
.form_submit {
  color: #25232E;
  text-indent: -99999px;
  padding-bottom: 8%;
  background: url(data:image/png;base64,) no-repeat 50% 50% / contain;
  /*transition: all 0.3s ease-in;*/
}
.form_submit.form_submit_hover {
  background: url(data:image/png;base64,) no-repeat 50% 50% / contain;
}

/* 自定义提示 */
.toast_mask {
  position: fixed;
  top: 0;left: 0;
  z-index: 50;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.5);
}
.toast_area {
  position: absolute;
  top: 25%;
  left: 13%;
  z-index: 100;
  width: 73%;
  height: 17%;
  color: #fff;
  font-size: 30rpx;
  padding: 20px 0;
  text-align: center;
  background: url(data:image/png;base64,) no-repeat 50% 50% / contain;
}
.toast_area.hide {
  display: none!important;
}
.toast_icon {
  width: 70%;
  height: 54%;
  margin: 3% auto 5%;
  /*background-color: red!important;*/
}
.toast_icon .img {
  display: block;
  width: 34%;
  height: 100%!important;
  margin: 0 auto;
  -webkit-animation: circleLoading 2s linear infinite;
}
@-webkit-keyframes circleLoading  {
  0% {transform: rotate(0deg)}
  100% {transform: rotate(360deg)}
}
.toast_icon.loading {
  
}
.toast_icon.tips {
  background: url(data:image/png;base64,) no-repeat 50% 50% / contain;
}
.toast_icon.phone {
  background: url(data:image/png;base64,) no-repeat 50% 50% / contain;
}
.toast_icon.user {
  background: url(data:image/png;base64,) no-repeat 50% 50% / contain;
}

.meteor1 {
  position: absolute;
  top: 0;left: 0;
  z-index: 1;
  width: 40px;
  height: 40px;
  opacity: 0;
  -webkit-animation: meteor1 7s linear 2s infinite;
}
@-webkit-keyframes meteor1{
  0% {
    -webkit-transform: translate(1000%, -100%);
    opacity: 0.5;
  }
  40% {
    -webkit-transform: translate(-100%, 1000%);
    opacity: 1
  }
  100% {
    -webkit-transform: translate(-100%, 1000%);
    opacity: 1
  }
}
.meteor2 {
  position: absolute;
  top: 0;left: 0;
  opacity: 0;
  z-index: 1;
  width: 40px;
  height: 40px;
  -webkit-animation: meteor2 7s linear 2s infinite;
}
@-webkit-keyframes meteor2{
  0% {
    -webkit-transform: translate(500%, -100%);
    opacity: 0.5;
  }
  60% {
    -webkit-transform: translate(500%, -100%);
    opacity: 1
  }
  100% {
    -webkit-transform: translate(-500%, 1000%);
    opacity: 1
  }
}